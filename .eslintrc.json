{
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended"
  ],
  "rules": {
    "react/no-unescaped-entities": "off",
    "react/prop-types": "off",
    "@next/next/no-img-element": "off",
    "no-unused-vars": "warn",
    "no-console": "warn",
    "no-undef": ["error", { "typeof": true }],
    "no-useless-escape": "warn"
  },
  "globals": {
    "Set": "readonly",
    "Map": "readonly"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}